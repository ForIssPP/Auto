{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        "react-hot-loader/babel",
        ["@babel/transform-runtime", {
            "helpers": true,
            "regenerator": true
        }],
        ["@babel/plugin-proposal-class-properties", {
            "loose": true
        }],
        ["@babel/plugin-syntax-dynamic-import"],
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "lib"
        }, "ant"],
        ["import", {
            "libraryName": "antd-mobile",
            "libraryDirectory": "lib"
        }, "antd-mobile"]
    ]
}